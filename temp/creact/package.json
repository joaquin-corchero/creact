{
  "name": "creact",
  "version": "1.0.0",
  "description": "Dotnet Core, React, WebPack, Babel, Enzyme, Mocha app",
  "main": "index.js",
  "repository": "https://github.com/joaquin-corchero/creact",
  "author": "Joaquin Corchero",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --recursive",
    "test-watch": "npm test -- --watch",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js --env.prod & webpack -p",
    "build-react": "webpack --env.prod",
    "build-css": "node node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js --env.prod",
    "build-watch": "node node_modules/webpack/bin/webpack.js --watch"
  },
  "devDependencies": {
    "aspnet-webpack": "^1.0.29",
    "aspnet-webpack-react": "^2.0.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "3.3.7",
    "chai": "^4.1.0",
    "chai-enzyme": "^0.8.0",
    "css-loader": "0.28.1",
    "enzyme": "^2.9.1",
    "event-source-polyfill": "0.0.9",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.2.1",
    "jsdom": "^11.1.0",
    "json-loader": "0.5.4",
    "mocha": "^3.4.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.4.1",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "validator": "^8.0.0",
    "webpack": "2.5.1",
    "webpack-hot-middleware": "2.18.0"
  }
}
